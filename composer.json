{
    "name": "netgen/symfony-skeleton",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "netgen/symfony-skeleton-dev-pack": "1.0.x-dev"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "netgen/git-hooks": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
